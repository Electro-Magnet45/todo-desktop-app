{
  "name": "todo",
  "version": "0.1.2",
  "private": true,
  "repository": "https://github.com/Electro-Magnet45/todo-test-app.git",
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "electron-is-dev": "^2.0.0",
    "electron-updater": "^4.3.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.3.0",
    "web-vitals": "^1.1.2"
  },
  "main": "public/main.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev-start": "electron . --trace-warnings",
    "electron-ship": "electron-builder -c.extraMetadata.main=build/main.js build --win --publish always",
    "gadd": "git add .",
    "gpush": "git push -u origin master",
    "commit-prj": "npm run gadd && git commit -m \"%npm_config_commitnm% skip-publish\" && npm run gpush",
    "publish-prj": "npm run gadd && git commit -m \"%npm_config_commitnm%\" && npm run gpush"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^14.0.1",
    "electron-builder": "^22.11.7",
    "react-scripts": "^4.0.3"
  }
}
